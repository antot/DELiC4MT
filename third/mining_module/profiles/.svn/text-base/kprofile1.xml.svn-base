<?xml version="1.0" encoding="utf-8"?>

<Kybot id="used-against">
 <variables>
   <var name="X" type="term" lemma="the"/>
   <var name="Y" type="term" lemma="great"/>
   <var name="Z" type="term" lemma="! potential"/>
 </variables>

 <relations>
   <!-- X Y? Z -->
   <root span="X"/>
   <rel span="Y" pivot="X" direction="following" immediate = "true" opt="true"/>
   <rel span="Z" pivot="Y" direction="following" immediate = "true"/>
 </relations>

 <facts>
   <fact id='used-against-fact'>
     <source value='$Y/@lemma'/>
     <target value='$Z/@lemma'/>
   </fact>
 </facts>
</Kybot>

